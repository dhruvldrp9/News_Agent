
{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "env": {
    "OPENAI_API_KEY": "@openai_api_key",
    "GOOGLE_NEWS_API_KEY": "@google_news_api_key",
    "ELEVENLABS_API_KEY": "@elevenlabs_api_key",
    "SECRET_KEY": "@secret_key"
  }
}
